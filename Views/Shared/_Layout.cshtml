    <footer class="border-top footer text-muted" style="text-align:center;">
        <div class="container">
            &copy; 2020 - SpotifyNewMusic - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
    <script language="javascript">
        function rollOver(target) {
            $("#contextMenuLogIn").children().each(function (i, e) {
                $(e).css("border-bottom", "0px");
            });
            $("#brandsArea").children().each(function (i, e) {
                $(e).css("border-bottom", "0px");
            });
            $(target).css("border-bottom", "4px solid rgba(0,0,255,0.7)");
        }
        function logIn(target) {
            if ($(target).hasClass("menuItemsDisabled")) {
                return;
            }
            var url = '@Url.Action("SignIn", "Account", new { brand = "__TARGET__" })';
            url = url.replace("__TARGET__", $(target).attr("id"));
            url += "&brands=" + '@(Html.Raw(JsonSerializer.Serialize(brands)))';
            window.location.href = encodeURI(url);
            return false;
        }
        window.addEventListener("load", function () {
            $(window).resize();
        });
        $(window).resize(function () {
            $(".container").css("max-width", "100%");
            $(".container").css("max-height", "100%");
            $("#brandsArea").css("left", ($(window).width() - 270).toString() + "px");
            var menuLeft = $("#brandsArea").css("left");
            $("#contextMenuLogIn").css("left", menuLeft);
            $("#contextMenuLogIn").css("top", $("header").first().css("height"));
            if ($(window).width() < 376) {
                $("#brandsArea").css("position", "");
                $("#contextMenuLogIn").css("top", ($("#brandsArea").top + $("#brandsArea").height).toString() + "px");
            }
            $("#contextMenuLogIn").children().each(function (i, e) {
                $(e).css("padding", "10px 5px 10px 5px");
            });
            if ($.trim($("#brandsArea").html()) == "") {
                $("#brandsArea").append($("#@brands.Where(b => b.Key== defaultBrand).FirstOrDefault().Key"));
            }
            $("#brandsArea").attr("title", "@string.Join(",", brands.Where(b => b.Value==true).Select(b => b.Key))");
            if ($.trim($("#brandsArea").attr("title")) != "") {
                $.each($("#brandsArea").attr("title").split(','), function (i, e) {
                    $("#" + e).addClass("menuItemsDisabled");
                    $("#" + e).css("background-color","#cccccc");
                });
            }
        });
        $(function () {
            $('#brandsArea').mouseenter(function () {
                $('#contextMenuLogIn').css("display", "inherit");
            });
            $('#contextMenuLogIn').mouseenter(function () {
                $('#contextMenuLogIn').css("display", "inherit");
            });
            $('#contextMenuLogIn').mouseleave(function () {

                $('#contextMenuLogIn').css("display", "none");
            });
            $('#brandsArea').mouseleave(function () {
                var includes = (event.clientX > $("#contextMenuLogIn").offset().left && event.clientX < $("#contextMenuLogIn").offset().left + $("#contextMenuLogIn").width());
                var includes = includes & (event.clientY > ($("#contextMenuLogIn").offset().top - 40) && event.clientY < $("#contextMenuLogIn").offset().top + $("#contextMenuLogIn").height());
                if (!includes) {
                    $('#contextMenuLogIn').css("display", "none");
                }
            });
            $(".navbar-toggler").on("click", function () {
                if ($(".navbar-collapse").css("display") == "none") {
                    $("#brandsArea").css("display","inline-block");
                }
                else {
                    $("#brandsArea").css("display", "none");
                }
            });
        });
    </script>
</body>
</html>
